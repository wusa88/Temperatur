
set title "'Temperaturverlauf am'%d"
set xdata time
set timefmt "%d.%m.%Y_%H:%M:%S"
set format x "%H:%M"
set ylabel 'Grad Celsius'
set yrange [-5:100]
plot '<sqlite3 /var/www/db.sqlite "SELECT * FROM (SELECT id, zeit, temp1 FROM temperatur ORDER BY id DESC LIMIT 4) as tmp ORDER BY tmp.id ASC;"' using 0:1 with lines title 't1'
set output "temperatur.jpg"
set terminal jpeg
replot



